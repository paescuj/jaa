{
  "name": "jaa",
  "description": "Job Application Assistant",
  "private": true,
  "scripts": {
    "dev": "react-env --prefix NEXT_PUBLIC --path .env.development -- next dev",
    "build": "next build",
    "start": "react-env --prefix NEXT_PUBLIC --path .env.production -- next start",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "pnpm run lint --fix"
  },
  "dependencies": {
    "@beam-australia/react-env": "3.1.1",
    "@chakra-ui/react": "2.4.9",
    "@directus/sdk": "10.3.1",
    "@dnd-kit/core": "6.0.7",
    "@dnd-kit/sortable": "7.0.2",
    "@dnd-kit/utilities": "3.2.1",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@fontsource/quicksand": "4.5.12",
    "@react-page/editor": "5.4.1",
    "@react-page/plugins-slate": "5.4.1",
    "@tippyjs/react": "4.2.6",
    "animate.css": "4.1.1",
    "chakra-react-select": "4.4.3",
    "date-fns": "2.29.3",
    "framer-motion": "9.0.2",
    "iconoir-react": "6.2.1",
    "language-icons": "0.3.0",
    "lodash.isequal": "4.5.0",
    "next": "13.1.6",
    "object-path": "0.11.8",
    "object-traversal": "1.0.1",
    "omgopass": "3.2.1",
    "pullstate": "1.25.0",
    "quick-score": "0.2.0",
    "react": "18.2.0",
    "react-big-calendar": "1.6.4",
    "react-datepicker": "4.10.0",
    "react-dom": "18.2.0",
    "react-flip-toolkit": "7.0.17",
    "react-hook-form": "7.43.1",
    "react-intl": "6.2.8",
    "react-measure": "2.5.2",
    "react-pdf": "6.2.2",
    "swiper": "9.0.4",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@types/react": "18.0.28",
    "eslint": "8.34.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "prettier": "2.8.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/core",
        "typescript",
        "@chakra-ui/*",
        "@mui/material",
        "slate-react-presentation"
      ],
      "allowedVersions": {
        "react": "18"
      }
    }
  }
}
